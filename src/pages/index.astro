---
import Layout from "../layouts/Layout.astro";
import Skills from "../components/Sections/Skills.astro";
import About from "../components/Sections/About.astro";
import Certifications from "../components/Sections/Certifications.astro";
import Interests from "../components/Sections/Interests.astro";
const pageTitle = "Emma The Possum";
---

<Layout pageTitle={pageTitle}>
  <div class="gif">
    <img
      width="323"
      height="294"
      alt="Excited Opossum Jumping Gif. Source: https://giphy.com/stickers/percolate-galactic-opossum-possum-VFwShkA0q2fj6tjzUx"
      src="/possum.gif"
    />
  </div>

  <h1>EmmaThePossum</h1>
  <span class="badge">💻 Full Stack Developer</span>
  <span class="badge">🐶 Dog Mom</span>
  <span class="badge">🌱 Vegan</span>

  <About />

  <Skills />

  <Certifications />

  <Interests />
</Layout>
